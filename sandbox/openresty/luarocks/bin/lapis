#!/bin/sh

LUAROCKS_SYSCONFDIR='/sandbox/openresty/luarocks/etc/luarocks' LUA_INIT='package.path="/sandbox/openresty/luarocks/share/lua/5.1/?.lua;/sandbox/openresty/luarocks/share/lua/5.1/?/init.lua;"..package.path;package.cpath="/sandbox/openresty/luarocks/lib/lua/5.1/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("lapis","1.7.0-1")' exec '/sandbox/openresty/luajit/bin/luajit' '/sandbox/openresty/luarocks/lib/luarocks/rocks-5.1/lapis/1.7.0-1/bin/lapis' "$@"
